{
    "name": "laravel-appkit/ui",
    "description": "",
    "keywords": [
        "laravel",
        "appkit",
        "ui"
    ],
    "homepage": "https://github.com/laravel-appkit/ui",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Darren Coutts",
            "email": "d@coutts.io",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.38.0|^9.0|^10.0"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.1",
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.6",
        "pestphp/pest": "^1.23"
    },
    "autoload": {
        "psr-4": {
            "AppKit\\UI\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppKit\\UI\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AppKit\\Ui\\UiServiceProvider"
            ],
            "aliases": {
                "Ui": "AppKit\\Ui\\Facades\\Ui"
            }
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
